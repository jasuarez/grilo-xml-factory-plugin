<source api="1">
  <id>xml-test-log</id>
  <name>XML Test Log</name>

  <operation debug="true">
    <browse>
      <result>
        <![CDATA[
                 <data>
                 <artist>My Artist</artist>
                 <title>My Title</title>
                 </data>
        ]]>
